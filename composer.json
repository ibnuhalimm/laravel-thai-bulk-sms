{
    "name": "ibnuhalimm/laravel-thai-bulk-sms",
    "description": "Thai Bulk SMS SDK for Laravel",
    "keywords": [
        "ibnuhalimm",
        "laravel-thai-bulk-sms",
        "thai-bulk-sms",
        "sms"
    ],
    "homepage": "https://github.com/ibnuhalimm/laravel-thai-bulk-sms",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ibnu Halim Mustofa",
            "email": "ibnuhalimm@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ibnuhalimm\\LaravelThaiBulkSms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ibnuhalimm\\LaravelThaiBulkSms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ibnuhalimm\\LaravelThaiBulkSms\\ServiceProvider"
            ],
            "aliases": {
                "LaravelThaiBulkSms": "Ibnuhalimm\\LaravelThaiBulkSms\\Facade"
            }
        }
    }
}
